{
  "name": "buildly-ui-react",
  "version": "0.0.1",
  "description": "Core frontend module for applications built using the React framework",
  "main": "src/index.js",
  "private": true,
  "workspaces": [
    "src/clients/*"
  ],
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@buildlyio/freyja-react": "^0.1.1",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.2.0",
    "gulp-git": "^2.9.0",
    "i18next": "^15.1.3",
    "i18next-browser-languagedetector": "^3.0.1",
    "jest-styled-components": "^6.3.1",
    "npm": "^6.9.0",
    "polished": "^3.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-i18next": "^10.11.0",
    "react-notifications": "^1.4.3",
    "react-redux": "^7.1.0",
    "react-router-dom": "^4.4.0-beta.8",
    "react-styleguidist": "^9.0.8",
    "react-test-renderer": "^16.8.6",
    "reactjs-popup": "^1.4.0",
    "redux-saga": "^1.0.0",
    "serve": "^11.0.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.13",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^4.2.0",
    "gulp-header": "^2.0.7",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^23.6.0",
    "midgard-core": "^0.1.4",
    "node-sass": "^4.11.0",
    "react-docgen": "^4.1.1",
    "react-docgen-typescript": "^1.12.4",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "test": "jest  --watch",
    "test-prod": "jest",
    "test-coverage": "jest  --updateSnapshot --coverage",
    "serve": "./node_modules/.bin/serve -s dist",
    "build": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "createFile": "yarn run cleanUp && gulp createFile  --gulpfile gulpfile.js --cwd ./",
    "init": "gulp init --gulpfile gulpfile.js --cwd ./ && yarn run createFile",
    "cleanUp": "gulp deleteIndex  --gulpfile gulpfile.js --cwd ./",
    "docs": "./node_modules/typedoc/bin/typedoc --tsconfig tsconfig.json",
    "styleguidist": "npx styleguidist build && npx styleguidist server"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src",
      "src",
      "<rootDir>/src/styles",
      "<rootDir>"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "\\.(js|jsx)?$": "./test-transform.js"
    },
    "setupFiles": [
      "<rootDir>/test-setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,tsx}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "keywords": [
    "Buildly",
    "Buildlyio",
    "React"
  ],
  "license": "ISC",
  "repository": "https://github.com/buildlyio/buildly-ui-react.git",
  "author": "Andrew Shortall"
}
